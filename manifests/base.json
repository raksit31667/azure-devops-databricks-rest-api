{
    "manifestVersion": 1,
    "id": "azure-devops-databricks-rest-api",
    "name": "Azure Databricks REST API",
    "icons": {
        "default": "icon.png"
    },
    "version": "{{version}}",
    "description": "Interacting with Azure Databricks via REST API",
    "publisher": "Raksit Mantanacharu",
    "public": true,
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "ado-databricks-rest-api-verifyalljobsrunning",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "verify-all-jobs-running"
            }
        },
        {
            "id": "ado-databricks-rest-api-updateinstancepools",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "update-instance-pools"
            }
        },
        {
            "id": "ado-databricks-rest-api-cancelautomatedjobruns",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "cancel-automated-job-runs"
            }
        }
    ],
    "files": [
        {
            "path": "verify-all-jobs-running",
            "addressable": true
        },
        {
            "path": "update-instance-pools",
            "addressable": true
        },
        {
            "path": "cancel-automated-job-runs",
            "addressable": true
        }
    ]
}